{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {},
	"rules": {
		"no-console": false,
		"indent": [
			true,
			"tabs",
			4
		],
		"quotemark": [
			true,
			"single",
			"avoid-escape",
			"avoid-template"
		],
		"trailing-comma": [
			true,
			{
				"singleline": "never",
				"multiline": {
					"objects": "always",
					"arrays": "always",
					"functions": "never",
					"typeLiterals": "ignore"
				}
			}
		]
	},
	"rulesDirectory": []
}